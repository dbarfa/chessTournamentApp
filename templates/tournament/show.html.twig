{% extends 'base.html.twig' %}

{% block body %}


    <table class="table table-hover  table-light ">

        <thead>
        <tr>
            <th class="col">#</th>
            <th class="col">username</th>
            <th class="col">First Name</th>
            <th class="col">Last Name</th>
            <th class="col">Birth Date</th>
            <th class="col">Elo</th>

            <th class="col">sex</th>

        </tr>
        </thead>
        <tbody>

        {% for p in print[0].players %}
            <tr>


                {#                {{ dump(p.players) }}#}


                <td class="col">{{ p.id }}</td>
                <td class="col">{{ p.username }}</td>
                <td class="col">{{ p.firstName }}</td>
                <td class="col">{{ p.lastName }}</td>
                <td class="col">{{ p.birthDate | date('d-m-Y') }}</td>
                <td class="col">{{ p.elo }}</td>
                <td class="col">{{ p.sex.value }}</td>

            </tr>

        {% endfor %}


        </tbody>


    </table>




{% endblock %}

{% block title %}

{% endblock %}

